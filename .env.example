# Cloud provider API keys
FIRECRAWL_API_KEY=your_firecrawl_key_here
TAVILY_API_KEY=your_tavily_key_here
BRAVE_API_KEY=your_brave_key_here
EXA_API_KEY=your_exa_key_here
MXB_API_KEY=your_mixedbread_key_here

# Jina (opt-in only via -jina flag â€” token-based billing is significantly
# more expensive than other providers for equivalent operations)
# JINA_API_KEY=your_jina_key_here

# Firecrawl rate limiting (requests per minute, default: 5 for free tier)
# Set higher for paid plans, or 0 to disable.
# FIRECRAWL_RATE_LIMIT=5

# Firecrawl retry headroom (defaults shown)
# FIRECRAWL_MAX_RETRIES=5
# FIRECRAWL_RETRY_INITIAL_BACKOFF=1s
# FIRECRAWL_RETRY_MAX_BACKOFF=90s

# Optional: scoring diagnostics (required if -quality flag is used)
# EMBEDDING_MODEL_BASE_URL=https://api.provider.com/v1
# EMBEDDING_MODEL_API_KEY=your_key
# EMBEDDING_MODEL=Qwen/Qwen3-Embedding-8B
# RERANKER_MODEL_BASE_URL=https://api.provider.com/v1
# RERANKER_MODEL_API_KEY=your_key
# RERANKER_MODEL=Qwen/Qwen3-Reranker-8B
